{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "app.py": {
      "includeFiles": "LLC_Data_Optimized.csv,LLC_Data_Optimized.json,LLC Data.csv",
      "maxDuration": 30
    }
  }
}
